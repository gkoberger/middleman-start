#sponsors
  [class^=logo-]
    width 150px
    height (58px * 0.75)
    display inline-block
    background-image url(/img/techfair/logos.png?5)
    opacity 0.7
    margin 0 15px
    vertical-align middle
    margin-bottom 15px
    margin-top 13px
    background-size auto (550px * 0.75)
    @media (max-width: 820px)
      height 29px
      background-size auto 275px 
      margin 0 10px
      margin-bottom 11px
      margin-top 6px
    logoer(left top width)
      background-position (left * 0.75) (top * 0.75)
      width width * 0.75
      @media (max-width: 820px)
        width (width / 2px)
        background-position (left / 2) (top / 2)
    &:hover
      opacity 1
    &.logo-backplane 
      logoer -21px -146px 288px
    &.logo-betable 
      logoer -341px -146px 239px
    &.logo-cloudera 
      logoer -614px -146px 238px

    &.logo-paloaltonetworks
